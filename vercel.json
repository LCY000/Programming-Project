{
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "body-parser": {
        "sizeLimit": "10mb"
      },
      "timeout": 10
    }
  ]
}